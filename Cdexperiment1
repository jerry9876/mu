#include <stdio.h>
#include <string.h> 
#include <stdlib.h>

char a[100]; 
int n, i;

int main()
{
  clrscr();
  printf("\nenter string");   
  scanf("%s",&a); 
  n=strlen(a);

  if(a[0] == 'a' && (a[n-1] == 'a' || a[n-1] == 'b') && a[n-2] == 'c'){
    for(int i=1; i<n-2 ; i++){
      if(a[i]!='b'){
        printf("\n String is not Accepted ");
        getch();
        exit(0);
      }
    }
    printf(" String is Accepted "); 
  }

  else{
    printf("\n String is not Accepted ");
  }


  getch();

}

